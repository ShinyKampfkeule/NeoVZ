<h1>Bearbeiten</h1>

<form method="post" action="/edit/save">
    <input type="hidden" name="id" value="{{id}}">
    {{#each data}}
        <label>{{this.label}}</label>
        <input type="{{this.type}}" name="{{this.name}}" value="{{this.value}}">
        <br>
        <br>
    {{/each}}
    <button type="submit">Save</button>
</form>